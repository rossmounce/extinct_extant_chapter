  x[ii, ] <- c(i, j, treedist(tr2,ref2[[j]]))
  ii <- ii+1
}
else if(class(tr2)=="multiPhylo" & class(ref2)=="phylo") {
  x[ii, ] <- c(i, j, treedist(tr2[[i]],ref2))
  ii <- ii+1
}
else if(class(tr2)=="multiPhylo" & class(ref2)=="multiPhylo") {
  x[ii, ] <- c(i, j, treedist(tr2[[i]],ref2[[j]]))
  ii <- ii+1
    }
